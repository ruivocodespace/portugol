programa 
{
  funcao inicio() 
  {
    //Declaracao de variaveis
    inteiro numero [5]
    inteiro contador
      //Coleta os numeros e coloca no vetor
      para (contador = 0; contador < 5; contador++){
        escreva("Insira um numero inteiro: ")
        leia(numero[contador])
      }
      //Imprime os numeros do primeiro ao ultimo digitado
      para (contador = 0; contador < 5; contador++){
        escreva(numero[contador], " ")
      }
      //Imprime os numeros do ultimo ao primeiro digitado
      para (contador = 4; contador >=0; contador--){
        escreva(numero[contador], " ")
      } 
  }
}
